{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "backpack/crud": "^4.1",
        "backpack/pagemanager": "^3.0",
        "backpack/revise-operation": "^1.0",
        "capeandbay-devs/nautical-integrator": "dev-master",
        "consoletvs/charts": "^7.2",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "goldspecdigital/laravel-eloquent-uuid": "^8.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "ixudra/curl": "^6.21",
        "laravel/framework": "^8.40",
        "laravel/tinker": "^2.5",
        "league/flysystem-aws-s3-v3": "~1.0",
        "lorisleiva/laravel-actions": "^2.1",
        "ramsey/uuid": "^4.1",
        "silber/bouncer": "v1.0.0-rc.10",
        "spatie/laravel-activitylog": "^4.0",
        "spatie/laravel-event-sourcing": "^5.0"
    },
    "require-dev": {
        "backpack/generators": "^3.1",
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laracasts/generators": "^2.0",
        "laravel/sail": "^1.0.1",
        "laravel/ui": "^3.3",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "helpers/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "path",
            "url": "packages/capeandbay-devs/nautical-integrator"
        }
    ]
}
